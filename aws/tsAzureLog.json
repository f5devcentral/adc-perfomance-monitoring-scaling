{
    "class": "Telemetry",
    "controls": {
      "class": "Controls",
      "logLevel": "debug"
    },
    "My_Poller": {
      "class": "Telemetry_System_Poller",
      "interval": 60,
      "actions": [{         
         "includeData": {},
         "locations": {
             "avr": true  
         }    
       }]
    },
    "My_Listener": {
      "class": "Telemetry_Listener",
      "port": 6514
    },
    "My_Azure_Consumer": {
       "class": "Telemetry_Consumer",
       "type": "Azure_Log_Analytics",
       "workspaceId": "${wrkspace_id}",
       "passphrase": {
           "cipherText": "${passphrase}"
       },
       "useManagedIdentity": false,
       "region": "${region}"
     }
 }
