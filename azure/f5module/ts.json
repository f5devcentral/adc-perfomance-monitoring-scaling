{
  "class": "Telemetry",
  "controls": {
    "class": "Controls",
    "logLevel": "debug"
  },
  "My_Poller": {
    "class": "Telemetry_System_Poller",
    "interval": 60
  },
  "My_Listener": {
    "class": "Telemetry_Listener",
    "port": 6514
  },
  "My_ELK_Consumer": {
    "class": "Telemetry_Consumer",
    "type": "Generic_HTTP",
    "host": "${logStashIP}",
    "protocol": "http",
    "port": 8080,
    "path": "/",
    "method": "POST",
    "headers": [
         {
           "name": "content-type",
           "value": "application/json"
         }
     ]
  },
  "My_Consumer": {
    "class": "Telemetry_Consumer",
    "type": "Splunk",
    "host": "206.124.129.187",
    "protocol": "https",
    "port": 8088,
    "allowSelfSignedCert": true,
    "passphrase": {
        "cipherText": "f02428fa-bc2e-42de-8368-ee25fe35ef5d"
    }
  }
}
