      "passphrase": {
          "cipherText": ""
      },
      "useManagedIdentity": false,
      "region": "westus"
    },
    "My_ELK_Consumer": {
       "class": "Telemetry_Consumer",
       "type": "Generic_HTTP",
       "host": "206.124.129.139",
